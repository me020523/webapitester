<jobs>
	<job id="openstack" info="com.ibm.cpi.watcher.job.openstack.JobInfo">
		<class name="com.ibm.cpi.watcher.job.openstack.FlavorTest" onFailure="destory_flavor">
			<case id="create_flavor" method="createFlavor" order="1"/>
			<case id="destory_flavor" method='deleteFlavor' order="4"/>
		</class>
		<class name="com.ibm.cpi.watcher.job.openstack.KeypairTest" onFailure="destory_keypair">
			<case id="create_keypair" method="createKeypair" order="2"/>
			<case id="destroy_keypair" method="deleteKeypair" order="3"/>
		</class>
	</job>
</jobs>